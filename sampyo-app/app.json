{
  "expo": {
    "name": "골재 분석 어플리케이션",
    "slug": "sampyo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#020326"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.snu.sampyo",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.snu.sampyo",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "MANAGE_DOCUMENTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
